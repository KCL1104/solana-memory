  "author": "AgentMemory Team",
  "license": "MIT",
  "peerDependencies": {
    "@elizaos/core": "^0.1.0",
    "agent-memory": "^1.0.0"
  },
  "devDependencies": {
    "@elizaos/core": "^0.1.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "agent-memory": "^1.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
