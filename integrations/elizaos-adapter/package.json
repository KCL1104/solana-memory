{
  "name": "@agent-memory/elizaos-adapter",
  "version": "1.0.0",
  "description": "ElizaOS Database Adapter for AgentMemory - Solana-based persistent storage",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
